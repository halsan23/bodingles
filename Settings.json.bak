{
	"trailingComma": "es5",
	"tabWidth": 4,
	"semi": false,
	"singleQuote": true,

	"workbench.colorCustomizations": {
		 "sideBar.foreground": "#c7c7c7",
		 "sideBar.background": "#080e0c",
		 "sideBarSectionHeader.background": "#0A1410",
		 "editor.background": "#111111",
		 "breadcrumb.background": "#0A1410",
		 "statusBar.background": "#0A1410",
		 "statusBar.foreground": "#626262",
		 "minimap.background": "#0A1410",
		 "titleBar.activeBackground": "#0b1014",
		 "editorGroupHeader.tabsBackground": "#0A1410",
		 "activityBar.background": "#0A1410",
		 "tab.activeBackground": "#1e3f32",
		 "tab.inactiveBackground": "#0A1410",
		 "tab.activeBorder": "#ffd866",
		 "editor.lineHighlightBackground": "#151a18"
	},
	"editor.tokenColorCustomizations": {
		 "textMateRules": [
			{
			  "scope": [
				 "string.quoted.double.html",
				 "meta.attribute.href.html",
				 "meta.tag.metadata.link.void.html"
			  ],
			  "settings": {
				 "foreground": "#9c8d6c"
			  }
			},
			{
			  "scope": [
				 "entity.name.tag.html",
				 "meta.tag.structure.header.start.html"
			  ],
			  "settings": {
				 "foreground": "#258b92"
			  }
			},
			{
			  "scope": ["comment.block.html"],
			  "settings": {
				 "foreground": "#657a5c"
			  }
			},
			{
			  "scope": [
				 "entity.other.attribute-name.html",
				 "meta.attribute.class.html",
				 "meta.tag.object.img.void.html"
			  ],
			  "settings": {
				 "foreground": "#b9665a"
			  }
			},
			{
			  "scope": [
				 "meta.property-value.css",
				 "meta.property-list.css",
				 "source.css"
			  ],
			  "settings": {
				 "foreground": "#5b5c2f"
			  }
			},
			{
			  "scope": ["keyword.other.unit.px.css"],
			  "settings": {
				 "foreground": "#ffffff"
			  }
			},
			{
			  "scope": [
				 "string.quoted.single.css",
				 "meta.property-value.css",
				 "meta.property-list.css",
				 "source.css",
				 "string.quoted.single.css"
			  ],
			  "settings": {
				 "foreground": "#b8b874"
			  }
			},
			{
			  "scope": ["constant.numeric.css"],
			  "settings": {
				 "foreground": "#ae6054"
			  }
			},
			{
			  "scope": [
				 "punctuation.definition.entity.html",
				 "constant.character.entity.named.nbsp.html",
				 "text.html.derivative"
			  ],
			  "settings": {
				 "foreground": "#b2b2b2"
			  }
			},
			{
			  "scope": ["source.css"],
			  "settings": {
				 "foreground": "#FFFF80"
			  }
			}
		 ]
	  },
	"editor.fontSize": 16,
	"editor.tabSize": 3,
	"editor.linkedEditing": true,
	"editor.wordWrap": "on",
	"liveServer.settings.donotShowInfoMsg": true,
	"editor.minimap.enabled": false,
	"explorer.confirmDelete": false,
	"html.hover.documentation": false,
	"scss.hover.documentation": false,
	"less.hover.documentation": false,
	"css.hover.documentation": false,
	"editor.detectIndentation": false,
	"files.trimTrailingWhitespace": true,
	"explorer.confirmDragAndDrop": false,
	"git.openRepositoryInParentFolders": "never",
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"workbench.iconTheme": "Monokai Pro (Filter Machine) Icons",
	"vsicons.dontShowNewVersionMessage": false,
	"window.titleBarStyle": "custom",
   "settingsSync.ignoredSettings": [],
	"editor.stickyScroll.scrollWithEditor": false,
	"workbench.tree.enableStickyScroll": false,
	"editor.stickyScroll.enabled": false,
	"emmet.triggerExpansionOnTab": true,
	"files.associations": {
	  "*html": "html",
	  "*njk": "html"
	},
	"emmet.useInlineCompletions": true,
	"emmet.includeLanguages": {
	  "javascript": "javascriptreact",
	  "typescript": "typescriptreact",
	  "vue-html": "html",
	  "vue": "html",
	  "razor": "html",
	  "plaintext": "pug",
	  "django-html": "html"
	},
	"emmet.showSuggestionsAsSnippets": true,
	"emmet.showExpandedAbbreviation": "always",
